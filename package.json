{
  "name": "mdx-cssx",
  "version": "0.0.4",
  "description": "CSS in MDX",
  "main": "dist/index.js",
  "source": "index.js",
  "scripts": {
    "prepublish": "npm run build",
    "dist:modules": "cross-env BABEL_ENV=modules babel index.js --out-dir ./dist",
    "build": "npm run dist:modules",
    "format": "prettier --write '**/*.{js,css,md}'"
  },
  "keywords": [
    "css",
    "mdx",
    "cssx"
  ],
  "author": "Sara Vieira",
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.8.0 || ^17"
  },
  "devDependencies": {
    "@babel/cli": "7.15.7",
    "@babel/core": "7.15.8",
    "@babel/eslint-parser":"^7.15.8",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-env": "7.15.8",
    "cross-env": "^5.2.0",
    "prettier": "^2.4.1"
  },
  "dependencies": {
    "@emotion/react": "^11.4.1"
  }
}
